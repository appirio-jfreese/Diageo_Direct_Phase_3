<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Where To Buy service custom settings</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Geo_Code_URL__c</fullName>
        <description>Google Geo Code API URL for getting coordinates of given address</description>
        <externalId>false</externalId>
        <label>Geo Code URL</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Locale__c</fullName>
        <description>Default Locale for searching stores</description>
        <externalId>false</externalId>
        <label>Locale</label>
        <length>20</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Num_Results__c</fullName>
        <defaultValue>15</defaultValue>
        <description>Total number of results returned in one request</description>
        <externalId>false</externalId>
        <label>Num Results</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Radius__c</fullName>
        <description>Default radius for getting nearest stores for given coordinates</description>
        <externalId>false</externalId>
        <label>Radius</label>
        <length>5</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>WTB_API_User_Id__c</fullName>
        <description>User id for WTB API user to authenticate the service request from authorized user</description>
        <externalId>false</externalId>
        <label>WTB API User Id</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>WTB_API_User_Value__c</fullName>
        <description>Value of WTB API user Id to authenticate the user</description>
        <externalId>false</externalId>
        <label>WTB API User Value</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>WTB Service Settings</label>
</CustomObject>
